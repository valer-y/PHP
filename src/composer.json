{
    "require": {
        "ramsey/uuid": "^4.7",
        "vlucas/phpdotenv": "^5.6",
        "psr/container": "^2.0",
        "symfony/mailer": "^6.4"
    },
    "autoload": {
      "psr-4": {
        "App\\": "app/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "config": {
      "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
