{
    "require": {
        "ext-pdo": "*",
        "vlucas/phpdotenv": "^5.3",
        "doctrine/dbal": "^3.3",
        "doctrine/orm": "^2.11",
        "symfony/cache": "^6.0",
        "doctrine/migrations": "^3.4",
        "twig/twig": "^3.8",
        "twig/intl-extra": "^3.8",
        "slim/slim": "^4.13",
        "slim/psr7": "^1.6",
        "slim/twig-view": "3.x-dev"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev"
}
